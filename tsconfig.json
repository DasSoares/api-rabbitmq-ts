{
    "compilerOptions": {
      "target": "ES2022", // corrige conflitos com objetos
      "lib": ["es2022", "ES5", "ES6"],
      "module": "NodeNext",
      // "moduleResolution": "node",
      "baseUrl": "src",
      "outDir": "dist",
      "sourceMap": true,
      "noImplicitAny": true,
      "experimentalDecorators": true,
    },
    // "include": ["src/**/*"],
    "include": ["./src/**/*.ts", "./test/**/*.ts"],
  "exclude": ["./node_modules/*", "dist"]
}